<?xml version='1.0' encoding='utf-8'?>
<widget id="com.different.axian" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <name>Axian</name>
    <description>
        Something vaguely approaching Namco's Galaxian arcade game.
    </description>
    <author email="alister@different.com" href="http://axian.different.com">
        Alister Lewis-Bowen
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="Fullscreen" value="true" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
